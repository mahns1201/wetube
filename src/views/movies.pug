extends templates/layout
include mixins/movies

block content
  video(src="https://parular-contents.s3.ap-northeast-2.amazonaws.com/duplicate/product/20211118_o7DP3Y557VnqyH4qV2h8Hh.mp4", controls)
  div
    button#play Play
    button#mute Mute
    input(type="range",step="0.1", value=0.5, min="0", max="1")#volume
    span#time 00:00/00:00

  h1=pageTitle
  ul
    each movie in movies
      li
        +movie(movie)

  script(src="/assets/js/videoPlayer.js")